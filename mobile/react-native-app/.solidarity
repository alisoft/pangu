{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": ">=14.15.1"
      }
    ],
    "Xcode": [
      {
        "rule": "cli",
        "binary": "xcodebuild",
        "version": "-version",
        "semver": ">=12.0.0",
        "platform": "darwin"
      }
    ],
    "CocoaPods": [
      {
        "rule": "cli",
        "binary": "pod",
        "version": "--version",
        "semver": ">=1.11.2",
        "platform": "darwin"
      }
    ]
  }
}
