  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build:client": "vue-cli-service build --dest dist/client",
    "build:node": "cross-env SSR=1 vue-cli-service build --dest dist/node",
    "build:server": "rimraf build/* && tsc -p src/node/tsconfig.json",
    "build": "rimraf dist/* && npm run build:client && npm run build:node",
    "server:prod": "pm2 start ecosystem.config.json",
    "server:dev": "nodemon",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "jest -i --colors --verbose --detectOpenHandles --config=jest.config.node.js",
    "test:watch": "jest -i --watchAll --config=jest.config.node.js",
    "lint": "vue-cli-service lint"
  },
