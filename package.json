    "concurrently": "^7.2.1",
    "husky": "^7.0.4",
    "lerna": "^5.5.0",
    "lint-staged": "^11.2.6",
    "sort-package-json": "^1.52.0"
  },
  "packageManager": "pnpm@7.5.0",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
